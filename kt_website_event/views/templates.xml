        <template id='open_day_register'>
            <t t-call='website.layout'>
                <div class="oe_structure">
                    <div class="carousel slide s_banner" id="myCarousel3" style="height: 320px;">
                        <ol class="carousel-indicators hidden">
                            <li class="active" data-slide-to="0" data-target="#myCarousel3"/>
                        </ol>
                        <div class="carousel-inner">
                            <div class="item bg-img-16 active" style="">
                                <div class="container">
                                    <div class="row content">
                                        <div class="carousel-content col-md-6 col-sm-12">
                                            <h2>Registration Form</h2>
                                            <h3>Fill the form to make registration</h3>
                                            <p>
                                                <a class="btn btn-success btn-large" href="/page/website.contactus">
                                                    Contact us
                                                </a>
                                            </p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="carousel-control left hidden" data-slide="prev" data-target="#myCarousel3"
                             href="#myCarousel3" style="width: 10%">
                            <i class="fa fa-chevron-left"/>
                        </div>
                        <div class="carousel-control right hidden" data-slide="next" data-target="#myCarousel3"
                             href="#myCarousel3" style="width: 10%">
                            <i class="fa fa-chevron-right"/>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-4">
                        <form t-attf-action="/open_day_register/#{slug(event)}/#{slug(ticket)}/#{quantity}"
                              method='POST'>
                            <div>
                                <label class="control-label" for="name">Name</label>
                                <span style='color:red'>
                                    <t t-esc='error_name'/>
                                </span>
                                <input type="char" name="name" class="form-control"
                                       t-att-value="post and post.get('name')"/>
                            </div>
                            <div>
                                <label class="control-label" for="email">Email</label>
                                <span style='color:red'>
                                    <t t-esc='error_email'/>
                                </span>
                                <input type="email" name="email" class="form-control"
                                       t-att-value="post and post.get('email')"/>
                            </div>
                            <div>
                                <label class="control-label" for="mobile">Mobile</label>
                                <span style='color:red'>
                                    <t t-esc='error_mobile'/>
                                </span>
                                <input type="char" name="mobile" class="form-control"
                                       t-att-value="post and post.get('mobile')"/>
                            </div>
                            <button type="submit" class="btn btn-primary btn-lg pull-right btn-margin">Proceed</button>
                            <div class="clearfix"/>
                        </form>
                    </div>
                </div>
                <div class="col-md-3 oe_structure" id="right_column">
                </div>
                <div class="oe_structure">
                </div>
            </t>
            <style>
                .btn-margin {margin:5px;}
            </style>
        </template>


        <template id="open_day_register_confirm">
            <t t-call="website.layout">
                <div id="wrap">
                    <div class="oe_structure"/>
                    <div class="container">
                        <h1>Thanks!</h1>
