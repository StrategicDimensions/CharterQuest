                                    <label class="control-label field-label" for="email">Your Email Address</label>
                                    <input type="text" name="email" id="email"
                                           class="field-input form-control ng-scope ng-pristine ng-invalid ng-invalid-required"
                                           required="1" onchange="email_validation()"/>
                                    <span style="display:none" class="font-red" id="email_info"/>


                                    <br/>
                                    <div class="g-recaptcha"
                                         data-sitekey="6LcGJRITAAAAAOYbNNzoMcoC78mYCaKRcj2gZNas"></div>
                                    <span id='message' class='font-red'>
                                        <b>
                                            <t t-esc='message'/>
                                        </b>
                                    </span>
                                    <br/>

                                    <button type="submit" class="btn btn-default btn-primary mb32 a-submit next-submit">
                                        Subscribe
                                        <span class="fa fa-long-arrow-right"/>
                                    </button>

                                </div>
                            </form>
                            <!-- Login -->>
                            <form action="/magazine/signin" method="post">

                                <div class="col-lg-4">

                                    <!--<p><font color="darkblue" style="font-size: 140%;" size="5px"><b>Login to Download Magazine/s</b></font></p>-->
                                    <label class="control-label" for="email">Email</label>
                                    <input type="text" name="email2" id="email2"
                                           class="field-input form-control ng-scope ng-pristine ng-invalid ng-invalid-required"
                                           onchange="email_validation2()" required="1"/>
                                    <span style="display:none" class="font-red" id="email_info2"/>

                                    <br/>
                                    <button type="submit"
                                            class="btn btn-default btn-primary mb32 a-submit next-submit2">Login
                                        <span class="fa fa-long-arrow-right"/>
                                    </button>

                                </div>
                                <div class="col-lg-4">
                                </div>
                                <!-- Login end-->
