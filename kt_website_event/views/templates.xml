            <link rel="stylesheet" href="//code.jquery.com/ui/1.11.4/themes/smoothness/jquery-ui.css"/>
            <script src="//code.jquery.com/ui/1.11.4/jquery-ui.js"/>

            <script>
                $(document).ready(function(){
                $(".next-submit2").on("click",function(){
                error=0;

                if($("#email2").val()=="")
                if (error == 0)
                {
                alert("Signin Email is mandatory");
                error++;
                }


                if(error!=0)
                return false;
                })

                });

                $(document).ready(function(){
                $(".next-submit").on("click",function(){
                error=0;
                if($("#fname").val()=="")
                if (error == 0)
                {
                alert("First Name is mandatory");
                error++;
                }

                if($("#email").val()=="")
                if (error == 0)
                {
                alert("Email is mandatory");
                error++;
                }


                if(error!=0)
                return false;
                })

                });

                function email_validation(){
                jQuery.ajax({
                url:"/subscriber/email_validation",
                method:"POST",
                data:{email:$('#email').val()},
                success:function(response){
                response = jQuery.parseJSON(response)
                if (response &amp;&amp; response.error){
                $('#email').val('')
                $('#email_info').text(response.error)
                $('#email_info').css('display','block')
                }
                else{
                $('#email_info').css('display','none')
                }
                }

                });
                }

                function email_validation2(){
                jQuery.ajax({
                url:"/subscriber/email_validation2",
                method:"POST",
                data:{email2:$('#email2').val()},
                success:function(response){
                response = jQuery.parseJSON(response)
                if (response &amp;&amp; response.error){
                $('#email2').val('')
                $('#email_info2').text(response.error)
                $('#email_info2').css('display','block')
                }
                else{
                $('#email_info2').css('display','none')
                }
                }

                });
                }


            </script>
        </template>


