        <template id='in_out_stock_wrapper' inherit_id='website_sale.products_item' name="Products item">

            <xpath expr="//div[@class='ribbon-wrapper']" position='attributes'>
                <attribute name="t-if">product.website_style_ids and request.env['product.style'].sudo().search([('name','=','Sale Ribbon')]).id in product.website_style_ids.ids</attribute>
            </xpath>

            <xpath expr="//div[@class='ribbon-wrapper']" position='after'>
                <t t-set="product" t-value="product.sudo()"/>
                <t t-if="product.qty_available >= 1">
                    <div class="ribbon-wrapper" style="right: 0px !important;width: 145px !important;height: 160px !important;top: 20px !important;display:block;z-index:auto !important">
                        <div class="ribbon btn btn-success" style="width: 160px;top: 36px;background:#4cae4c !important;color: #fff !important;border: 1px solid#fff !important;">In Stock</div>
                    </div>
                </t>
                <t t-else="">
                    <div class="ribbon-wrapper" style="right: 0px !important;width: 145px !important;height: 160px !important;top: 20px !important;display:block;z-index:auto !important">
                        <div class="ribbon btn btn-success" style="padding: 6px 26px;width: 160px;top: 40px;background:red !important;color: #fff !important;border: 1px solid#fff !important;"><center>Out Of Stock</center></div>
                    </div>
                </t>
            </xpath>

        </template>


