        <template id="reset_magazine_password" name="Reset_Magazine-password">
            <t t-call='website.layout'>
                <div class='oe_structure'></div>
                <div>
                    <h3>Reset Password ...</h3>
                    <div class='col-lg-4'></div>
                    <div class='col-lg-4'>
                        <form action='/magazine/reset_magazine_account' method='post'>
                            <label class="control-label" for="email">Your Email</label>
                            <input type="text" name="email" id='email' class="form-control" t-att-value='record.email'
                                   readonly='1'/>
                            <br/>
                            <label class="control-label" for="email">Your Name</label>
                            <input type="text" name="name" id='name' class="form-control" t-att-value='record.name'
                                   readonly='1'/>
                            <br/>
                            <label class=" control-label" for="password">Password</label>
                            <input type="password" name="password" id='password' class="form-control" required='1'/>
                            <label class="control-label" for="confirm_password">Confirm Password</label>
                            <input type="password" name="confirm_password" class="form-control"
                                   onchange='onchange_password(this)' required='1'/>
                            <br/>
                            <span id='password_info' style='display:none' class='font-red'>Passwords do not match</span>
                            <br/>
                            <input type="hidden" name="obj" class="form-control" t-att-value='obj'/>
                            <input type="hidden" name="link" class="form-control" t-att-value='link'/>
                            <button type='submit' class="btn btn-default btn-primary mb32 a-submit">Reset Password
                                <span class="fa fa-long-arrow-right"/>
                            </button>
                            <a class='a-padding' href="/magazine/signin">Back to Login</a>
                        </form>
                    </div>
                    <div class='col-lg-4'></div>
                </div>
                <div class='oe_structure'></div>
            </t>
            <script>
                function onchange_password(event){
                console.log('in onchnge_password');
                new_password = event.value;
                console.log(new_password);
                console.log($('#password').val());
                password = $('#password').val();
                if (password != new_password){
                event.value = ''
                $('#password_info').css('display','block');
                }
                }
            </script>
        </template>

