    <record id="view_cfo_team_form" model="ir.ui.view">
        <field name="name">cfo.team.form</field>
        <field name="model">cfo.teams</field>
        <field name="arch" type="xml">
            <form string="CFO Team">
                <sheet string="CFO Team">
                <div class="oe_title">
                    <h1>
                        <field name="name" required="1"/>
                    </h1>
                </div>
                <group>
                  <group>
                    <field name='cfo_comp' invisible="1"/>
                    <field name='cfo_competition_year' invisible="1"/>
                  </group>
                  <group></group>
                </group>
              </sheet>
            </form>
        </field>
    </record>
    
    <record id="view_cfo_team_list" model="ir.ui.view">
        <field name="name">cfo.team.list</field>
        <field name="model">cfo.teams</field>
        <field name="arch" type="xml">
            <tree string="CFO Team">
                <field name="name"/>
            </tree>
        </field>
    </record>
    
    <record model="ir.actions.act_window" id="action_cfo_team_jnr_2016">
        <field name="name">CFO Jnr Teams</field>
        <field name="res_model">cfo.teams</field>
        <field name="view_mode">tree,form</field>
        <field name="domain">[('cfo_comp', '=', 'CFO JNR'), ('cfo_competition_year', '=', '2016')]</field>
        <field name="context">{'default_cfo_comp': 'CFO JNR', 'default_cfo_competition_year': '2016'}</field>
    </record>
    
